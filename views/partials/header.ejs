<header>
    <nav class="z-depth-0 purple darken-4">
        <div class="nav-wrapper">

            <a href="#!" class="brand-logo r-logo"><%= title %></a>
            <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
            <ul class="right hide-on-med-and-down m-right">
                <% for(let i = 0; navLinks.length > i; i++) { %>
                    <li class="">
                        <a href="/<%=navLinks[i].link%>"><%= navLinks[i].name %></a>
                    </li>
                <% } %>
            </ul>
            <% if(isVisible) { %>
                <div class="login-wrapper">
                    <span><a href="api/login">Вход</a></span>
                </div>
            <% } %>
        </div>
    </nav>

    <ul class="sidenav" id="mobile-demo">
        <% for(let i = 0; navLinks.length > i; i++) { %>
        <li class="">
            <a href="/<%=navLinks[i].link%>"><%= navLinks[i].name %></a>
        </li>
        <% } %>
        <li id="btn-close" class="sidenav-close"><a href="#"><span class="material-icons">close</span></a></li>
    </ul>
</header>
