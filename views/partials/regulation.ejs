<div class="row">
    <% for(let i = 0; regulations.length > i; i++) { %>
    <div class="col s12 m6 l6 xl4">
        <div class="card-panel white">
            <img width="100%" src="<%=regulations[i].img%>" alt="Rising star">
            <h5><%=regulations[i].title%></h5>
            <div style="margin-bottom: 0" class="btn-wrapper row">
                <a href="<%=regulations[i].url%>" style="background-color: #4a148c" class="waves-effect waves-light btn btn-small">Читать</a>
                <a id="<%=regulations[i].url%>" href="/create-pdf" style="background-color: #4a148c" class="right waves-effect waves-light btn btn-small">Скачать</a>
            </div>
        </div>
    </div>
    <% } %>
</div>
